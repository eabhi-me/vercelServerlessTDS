{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Install updated requirements",
			"type": "shell",
			"command": "D:/TDS/.venv/Scripts/python.exe",
			"args": [
				"-m",
				"pip",
				"install",
				"--upgrade",
				"pip",
				"&&",
				"D:/TDS/.venv/Scripts/python.exe",
				"-m",
				"pip",
				"install",
				"-r",
				"requirements.txt"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}